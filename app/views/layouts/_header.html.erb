<header>
  <nav>
    <ul class="clearfix">
      <% if user_signed_in?%>
      <li class="drop_down links_header" id="user_header">
        <%=current_user.first_name + " " +current_user.last_name%>
        <ul class="sub-menu">
          <li><%=link_to "Dashboard",'#'%></li>
          <li><%= link_to "Logout",destroy_user_session_path,:method => :delete%></li>
        </ul>
      </li>
      <%else%>
      <li class="links_header"><%=link_to 'log in', new_user_session_path,id:"login_button"%></li>
      <%end%>
      <li class="links_header header_links"><%=link_to 'how it works', how_it_works_path%></li>
      <li class="links_header header_links"><%=link_to 'about boxd', about_path%></li>
      <li class="links_header header_links"><%=link_to 'our meals', our_meals_path%></li>
      <li class="links_header"><%=link_to image_tag( 0, alt:'BOXD'), index_path%></li>
    </ul>
  </nav>
</header>
